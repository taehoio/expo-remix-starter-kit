{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Remix Cloudflare Pages",
  "extends": "./base.json",
  "compilerOptions": {
    "types": [
      "@remix-run/cloudflare",
      "vite/client",
      "@cloudflare/workers-types/2023-07-01"
    ],
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "allowJs": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true
  }
}
