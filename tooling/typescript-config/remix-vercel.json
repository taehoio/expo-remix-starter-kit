{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Remix Vercel",
  "extends": "./base.json",
  "compilerOptions": {
    "types": ["@remix-run/node", "vite/client"],
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "allowJs": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "strict": true,
    "noImplicitAny": true
    }
}
